# MailChimp Configuration
# Copy this file to .env and replace with your actual values
# Never commit .env to version control

# MailChimp API Key (get from Account > Extras > API keys)
REACT_APP_MAILCHIMP_API_KEY=921071254cb0c140c84d517e77bed105-us10

# MailChimp Server Prefix (found at the end of your API key, e.g., us1, us6)
REACT_APP_MAILCHIMP_SERVER_PREFIX=us10

# Google Analytics Configuration
# Get your GA4 Measurement ID from https://analytics.google.com/
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# VITE_GTM_AUTH=your_auth_string
# VITE_GTM_PREVIEW=preview_id
# VITE_GTM_COOKIES_WIN=x

# MailChimp Audience ID (get from Audience > Settings > Audience name and defaults)
REACT_APP_MAILCHIMP_AUDIENCE_ID=cd7207c9ee

# MailChimp Signup Form URL (get from Audience > Signup forms > Share it)
REACT_APP_MAILCHIMP_SIGNUP_URL=https://your-account.us1.list-manage.com/subscribe/post?u=...

# Optional: Disqus Configuration (if you want to keep Disqus settings here too)
# REACT_APP_DISQUS_SHORTNAME=your-disqus-shortname
